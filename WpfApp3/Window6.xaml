<Window x:Class="WpfApp3.Window6"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp3"
        xmlns:Sync="clr-namespace:Syncfusion.Windows.Controls.Gantt;assembly=Syncfusion.Gantt.Wpf"
        xmlns:hc="https://handyorg.github.io/handycontrol"
        mc:Ignorable="d"
        Title="Window6" Height="450" Width="800">
    <Grid>
        <Sync:GanttControl x:Name="Gantt" ></Sync:GanttControl>
                          <hc:Carousel Width="600" Height="300">
            <TextBlock Width="600" Height="300" Text="&#xE600;" Style="{DynamicResource FontAwesome1}" Foreground="Gold" />
            <TextBlock Width="600" Height="300" Text="&#xE6B1;" Style="{DynamicResource FontAwesome1}"  Foreground="Green"/>
            <TextBlock  Width="600" Height="300" Text="&#xE6B2;" Style="{DynamicResource FontAwesome1}"  Foreground="Red"/>
            <TextBlock Width="600" Height="300" Text="&#xF000;" Style="{DynamicResource FontAwesome}" />
        </hc:Carousel>
           
    </Grid>
</Window>
